{
  "name": "outdoors",
  "version": "1.0.0",
  "description": "nature travel website",
  "main": "index.js",
  "scripts": {
    "watch:scss": "node-sass scss/main.scss css/style.css -w",
    "compile:scss": "node-sass scss/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 8 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.min.css --output-style compressed",
    "build:css": "npm-run-all compile:scss concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/m2paulc/Outdoors.git"
  },
  "keywords": [
    "outdoors"
  ],
  "author": "paulc",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/m2paulc/Outdoors/issues"
  },
  "homepage": "https://github.com/m2paulc/Outdoors#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "concat": "^1.0.3",
    "gulp-live-server": "0.0.31",
    "gulp-uglify": "^3.0.1",
    "gulp4": "^4.0.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^6.0.0"
  },
  "dependencies": {}
}
